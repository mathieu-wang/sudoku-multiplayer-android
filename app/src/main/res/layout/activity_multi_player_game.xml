                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="YOU" />

                    <TextView
                        android:id="@+id/opponent_rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Rating: LOW" />

                    <TextView
                        android:id="@+id/opponent_games"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Games: 800" />
                </LinearLayout>
            </LinearLayout>

